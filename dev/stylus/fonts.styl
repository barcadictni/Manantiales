fuente(nombre,miFuente)
	@font-face
		font-family nombre
		font-weight normal
		src url('../fonts/'+ miFuente +'.eot')
		src url('../fonts/'+ miFuente +'.eot?#iefix') format('embedded-opentype'),url('../fonts/'+miFuente+'.woff') format('woff'),url('../fonts/'+miFuente+'.ttf') format('truetype'),url('../fonts/'+miFuente+'.svg#'+miFuente) format('svg')

fuente(OpenSans400, OpenSans-Regular-webfont)

fuente(pristina,Pristina)

fuente(GothamXL, Gotham-XLight)

fuente(GothamBK,Gotham-Book)

fuente(GothamB, Gotham-Bold)

fuente(GothamL,Gotham-Light)

fuente(GothamM,Gotham-Medium)

fuente(GothamLIta, Gotham-LightIta)

